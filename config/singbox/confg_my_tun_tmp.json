{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  },
  
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9095",
      "external_ui": "/etc/sing-box/ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/MetaCubeX/yacd/archive/gh-pages.zip",
      "external_ui_download_detour": "🚀 优质节点",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "path": "/etc/sing-box/cache.db",
      "store_fakeip": false
    }
  },

  "dns": {
    "servers": [
      {
        "tag": "dns_direct",
        "address": "223.5.5.5",
        "address_strategy": "ipv4_only",
        "strategy": "ipv4_only",
        "detour": "🎯 全球直连"
      },
      {
        "tag": "dns_proxy",
        "address": "tls://8.8.8.8",
        "address_strategy": "ipv4_only",
        "strategy": "ipv4_only",
        "detour": "🚀 优质节点"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "dns_direct",
        "disable_cache": true
      },
      {
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "clash_mode": "Global",
        "server": "dns_proxy"
      },
      {
        "rule_set": "geosite-cn",
        "server": "dns_direct"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "dns_proxy"
      }
    ],
    "final": "dns_proxy",
    "strategy": "ipv4_only"
  },

  "inbounds": [
    {
      "type": "tun",
      "address": "10.0.0.1/30",
      "mtu": 9000,
      "auto_route": true,
      "auto_redirect": true,
      "strict_route": true,
      "stack": "system",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      }
    },
    {
      "type": "mixed",
      "listen": "0.0.0.0",
      "listen_port": 2080,
      "sniff": true,
      "users": []
    }
  ],

  "outbounds": [
    {
      "tag": "🚀 优质节点",
      "type": "selector",
      "outbounds": ["♻️ 自动选择", "🚌 付费高容", "♾️ 无限节点", "🌍 地区节点", "🎥 奈飞节点", "💬 GPT节点", "🐸 全节点选择", "🎯 全球直连"]
    },
    {
      "tag": "📹 高流量服务",
      "type": "selector",
      "outbounds": ["🚌 付费高容", "🚀 优质节点", "♾️ 无限节点", "🐸 全节点选择", "🎯 全球直连"]
