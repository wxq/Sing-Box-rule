      "type": "urltest",
      "outbounds": ["{all}"],
      "filter": [
        {
          "action": "include",
          "keywords": ["^(?!.*(香港|日本|美国|台湾|加坡|韩国|德国|法国|英国|荷兰|瑞士|瑞典|西班牙|加拿大|澳大)).*"]
